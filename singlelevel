package Com.sample;
class animal{
	public void eat() {
		System.out.println("i can eat");
	}
   
}
class Dog extends animal{
	public void bark() {
		System.out.println("i can bark");
	}
}

public class inheritance {
	public static void main(String[] args) {
		Dog d = new Dog();
		d.eat();
		d.bark();
	}

}
OUTPUT: 
i can eat
i can bark
